CREATE TABLE IF NOT EXISTS `portfolio` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `background_id` int(11) DEFAULT NULL,
    `thumbnail_id` int(11) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `background_id` (`background_id`),
    KEY `thumbnail_id` (`thumbnail_id`),
    CONSTRAINT `portfolio_ibfk_1` FOREIGN KEY (`background_id`) REFERENCES `file` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `portfolio_ibfk_2` FOREIGN KEY (`thumbnail_id`) REFERENCES `file` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `portfolio_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `portfolio_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `meta_title` varchar(256) DEFAULT NULL,
    `meta_desc` varchar(256) DEFAULT NULL,
    `title` varchar(512) DEFAULT NULL,
    `text` text DEFAULT NULL,
    `url` varchar(512) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `portfolio_id` (`portfolio_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `portfolio_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `portfolio_lang_ibfk_2` FOREIGN KEY (`portfolio_id`) REFERENCES `portfolio` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

INSERT INTO `portfolio` (`id`, `position`) VALUES (1,1);
INSERT INTO `portfolio` (`id`, `position`) VALUES (2,2);
INSERT INTO `portfolio_lang` (`portfolio_id`, `lang`, `active`, `url`, `meta_title`, `meta_desc`, `title`, `text`) VALUES (1, 'pl', 1, 'portfolio/nowy-styl-group', '', '', 'Nowy Styl Group', '');
