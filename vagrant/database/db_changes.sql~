ALTER TABLE `site_lang` ADD COLUMN `text_down` TEXT DEFAULT NULL;
ALTER TABLE `site` ADD COLUMN `view` VARCHAR(256) DEFAULT 'page/site';

CREATE TABLE IF NOT EXISTS `file_site` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `file_id` int(11) DEFAULT NULL,
    `site_id` int(11) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `file_id` (`file_id`),
    KEY `site_id` (`site_id`),
    CONSTRAINT `file_site_ibfk_1` FOREIGN KEY (`file_id`) REFERENCES `file` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `file_site_ibfk_2` FOREIGN KEY (`site_id`) REFERENCES `site` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

