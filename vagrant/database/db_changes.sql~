drop database if exists kambudata;
create database kambudata;
use kambudata;

CREATE TABLE IF NOT EXISTS `ci_sessions` (
        `id` varchar(40) NOT NULL,
        `ip_address` varchar(45) NOT NULL,
        `timestamp` int(10) unsigned DEFAULT 0 NOT NULL,
        `data` blob NOT NULL,
        PRIMARY KEY (id),
        KEY `ci_sessions_timestamp` (`timestamp`)
);

CREATE TABLE IF NOT EXISTS `language` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` INT DEFAULT 1,
    `deleted` tinyint(1) DEFAULT 0,
    `name` varchar(128),
    `code` varchar(2),
    `active` tinyint(1) DEFAULT '0',
    PRIMARY KEY (`id`),
    KEY `code` (`code`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `user` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` INT DEFAULT 1,
    `deleted` tinyint(1) DEFAULT 0,
    `display_language` varchar(2) DEFAULT NULL,
    `password` varchar(512) DEFAULT NULL,
    `login` varchar(512) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `display_language` (`display_language`),
    CONSTRAINT `user_ibfk_1` FOREIGN KEY (`display_language`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

INSERT INTO `language` (`name`, `code`) VALUES ('Polish', 'pl');
INSERT INTO `user` (login, password, display_language) VALUES ('admin', MD5('admin'), 'pl');


CREATE TABLE IF NOT EXISTS `folder` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `position` int(11) DEFAULT NULL,
  `position_modifier` int(11) DEFAULT NULL,
  `position_moved` int(11) DEFAULT NULL,
  `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted` tinyint(1) DEFAULT '0',
  `name` varchar(512) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;


CREATE TABLE IF NOT EXISTS `file` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `position` int(11) DEFAULT NULL,
  `position_modifier` int(11) DEFAULT NULL,
  `position_moved` int(11) DEFAULT NULL,
  `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted` tinyint(1) DEFAULT '0',
  `folder_id` int(11) DEFAULT NULL,
  `path` varchar(512) DEFAULT NULL,
  `original_name` varchar(512) DEFAULT NULL,
  `extension` varchar(512) DEFAULT NULL,
  `type` varchar(512) DEFAULT NULL,
  `size` int(11) DEFAULT NULL,
  `width` int(11) DEFAULT NULL,
  `height` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `folder_id` (`folder_id`),
  CONSTRAINT `file_ibfk_1` FOREIGN KEY (`folder_id`) REFERENCES `folder` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;

CREATE TABLE IF NOT EXISTS `file_lang` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted` tinyint(1) DEFAULT '0',
  `file_id` int(11) DEFAULT NULL,
  `lang` varchar(2) NOT NULL,
  `active` tinyint(1) NOT NULL,
  `title` varchar(512) DEFAULT NULL,
  `alt` varchar(512) DEFAULT NULL,
  `name` text,
  PRIMARY KEY (`id`),
  KEY `file_id` (`file_id`),
  KEY `lang` (`lang`),
  CONSTRAINT `file_lang_ibfk_1` FOREIGN KEY (`file_id`) REFERENCES `file` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `file_lang_ibfk_2` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB  DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

-- psk 27.07.2015

CREATE TABLE IF NOT EXISTS `site` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `slider_id` int(11) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `slider_id` (`slider_id`),
    CONSTRAINT `site_ibfk_1` FOREIGN KEY (`slider_id`) REFERENCES `file` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `site_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `site_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `meta_title` varchar(256) DEFAULT NULL,
    `meta_desc` varchar(256) DEFAULT NULL,
    `title` varchar(256) DEFAULT NULL,
    `url` varchar(512) DEFAULT NULL,
    `text` text DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `site_id` (`site_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `site_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `site_lang_ibfk_2` FOREIGN KEY (`site_id`) REFERENCES `site` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

DELETE FROM `site` WHERE `id` = 1;
INSERT INTO `site` (`id`, `position`) VALUES (1, 0);
INSERT INTO `site_lang` (`site_id`, `lang`, `active`, `title`, `url`) VALUES (1, 'pl', 1, 'Strona główna', '');

