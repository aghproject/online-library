ALTER TABLE `portfolio` ADD COLUMN `view` VARCHAR(256) DEFAULT 'page/portfolio';

CREATE TABLE IF NOT EXISTS `branch` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `branch_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `branch_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `title` varchar(512) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `branch_id` (`branch_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `branch_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `branch_lang_ibfk_2` FOREIGN KEY (`branch_id`) REFERENCES `branch` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `project_type` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `project_type_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `project_type_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `title` varchar(512) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `project_type_id` (`project_type_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `project_type_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `project_type_lang_ibfk_2` FOREIGN KEY (`project_type_id`) REFERENCES `project_type` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `technology` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `technology_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `technology_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `title` varchar(512) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `technology_id` (`technology_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `technology_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `technology_lang_ibfk_2` FOREIGN KEY (`technology_id`) REFERENCES `technology` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `branch_portfolio` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `branch_id` int(11) DEFAULT NULL,
    `portfolio_id` int(11) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `branch_id` (`branch_id`),
    KEY `portfolio_id` (`portfolio_id`),
    CONSTRAINT `branch_portfolio_ibfk_1` FOREIGN KEY (`branch_id`) REFERENCES `branch` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `branch_portfolio_ibfk_2` FOREIGN KEY (`portfolio_id`) REFERENCES `portfolio` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `project_type_portfolio` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `project_type_id` int(11) DEFAULT NULL,
    `portfolio_id` int(11) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `project_type_id` (`project_type_id`),
    KEY `portfolio_id` (`portfolio_id`),
    CONSTRAINT `project_type_portfolio_ibfk_1` FOREIGN KEY (`project_type_id`) REFERENCES `project_type` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `project_type_portfolio_ibfk_2` FOREIGN KEY (`portfolio_id`) REFERENCES `portfolio` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `technology_portfolio` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `technology_id` int(11) DEFAULT NULL,
    `portfolio_id` int(11) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `technology_id` (`technology_id`),
    KEY `portfolio_id` (`portfolio_id`),
    CONSTRAINT `technology_portfolio_ibfk_1` FOREIGN KEY (`technology_id`) REFERENCES `technology` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `technology_portfolio_ibfk_2` FOREIGN KEY (`portfolio_id`) REFERENCES `portfolio` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

-- psk 31.07.2015

CREATE TABLE IF NOT EXISTS `offer` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `view` varchar(256) DEFAULT 'page/offer_subpage',
    PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `offer_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `offer_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `meta_title` varchar(256) DEFAULT NULL,
    `meta_desc` varchar(256) DEFAULT NULL,
    `date` DATE DEFAULT NULL,
    `title` varchar(512) DEFAULT NULL,  
    `short_text` text DEFAULT NULL,
    `text` text DEFAULT NULL,
    `url` varchar(512) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `offer_id` (`offer_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `offer_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `offer_lang_ibfk_2` FOREIGN KEY (`offer_id`) REFERENCES `offer` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

