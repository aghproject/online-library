CREATE TABLE IF NOT EXISTS `slider` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `site_id` int(11) DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `site_id` (`site_id`),
    CONSTRAINT `slider_ibfk_1` FOREIGN KEY (`site_id`) REFERENCES `site` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `slider_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `slider_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `text` text DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `slider_id` (`slider_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `slider_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `slider_lang_ibfk_2` FOREIGN KEY (`slider_id`) REFERENCES `slider` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

ALTER TABLE `site_lang` ADD COLUMN `slider_header` VARCHAR(256) DEFAULT NULL;

ALTER TABLE `site` DROP FOREIGN KEY `site_ibfk_1`;
ALTER TABLE `site` CHANGE COLUMN `slider_id` `image_id` INT(11) DEFAULT NULL;
ALTER TABLE `site` ADD CONSTRAINT `site_ibfk_1` FOREIGN KEY (`image_id`) REFERENCES `file` (`id`) ON DELETE CASCADE ON UPDATE CASCADE;

