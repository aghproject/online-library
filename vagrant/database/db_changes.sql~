CREATE TABLE IF NOT EXISTS `profile` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `position` int(11) DEFAULT NULL,
    `position_modifier` int(11) DEFAULT NULL,
    `position_moved` int(11) DEFAULT NULL,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

CREATE TABLE IF NOT EXISTS `profile_lang` (
    `id` int(11) NOT NULL AUTO_INCREMENT,
    `last_changed_timestamp` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    `deleted` tinyint(1) DEFAULT 0,
    `profile_id` int(11) DEFAULT NULL,
    `lang` varchar(2) NOT NULL,
    `active` tinyint(1) NOT NULL,
    `title` varchar(512) DEFAULT NULL,
    `text` text DEFAULT NULL,
    PRIMARY KEY (`id`),
    KEY `profile_id` (`profile_id`),
    KEY `lang` (`lang`),
    CONSTRAINT `profile_lang_ibfk_1` FOREIGN KEY (`lang`) REFERENCES `language` (`code`) ON DELETE CASCADE ON UPDATE CASCADE,
    CONSTRAINT `profile_lang_ibfk_2` FOREIGN KEY (`profile_id`) REFERENCES `profile` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1;

